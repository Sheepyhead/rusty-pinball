  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'rusty-pinball'",
      "cargo": {
        "args": [
          "build",
          "--bin=rusty-pinball",
          "--package=rusty-pinball"
        ],
        "filter": {
          "name": "rusty-pinball",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "RUST_BACKTRACE": "1"
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'rusty-pinball'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=rusty-pinball",
          "--package=rusty-pinball"
        ],
        "filter": {
          "name": "rusty-pinball",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}